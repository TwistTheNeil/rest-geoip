{{define "ipAddressInfo"}}
{{template "head" .}}
<div class="container justify-content-center align-items-center">
  <div class="card">
    <div class="card-header">
      {{.IP}}
    </div>
    <div class="card-body">
      <table class="table table-borderless table-hover table-sm">
      <tbody>
        <tr>
          <td>
            Country
          </td>
          <td>
            {{.Country.ISOCode}}
          </td>
        </tr>

        <tr>
          <td>
            EU
          </td>
          <td>
            {{if .Country.IsInEuropeanUnion}}
              Yes
            {{else}}
              No
            {{end}}
          </td>
        </tr>

        <tr>
          <td>
            City
          </td>
          <td>
            {{range $key, $value := .City.Names}}
              {{$key}}:{{$value}}</br>
            {{end}}
          </td>
        </tr>

        <tr>
          <td>
            Latitude
          </td>
          <td>
            {{.Location.Latitude}}
          </td>
        </tr>

        <tr>
          <td>
            Longitude
          </td>
          <td>
            {{.Location.Longitude}}
          </td>
        </tr>

        <tr>
          <td>
            Time Zone
          </td>
          <td>
            {{.Location.TimeZone}}
          </td>
        </tr>

        <tr>
          <td>
            Approx. Zip Code
          </td>
          <td>
            {{.Postal.Code}}
          </td>
        </tr>

        <tr>
          <td>
            Anonymous Proxy
          </td>
          <td>
            {{.Traits.IsAnonymousProxy}}
          </td>
        </tr>

        <tr>
          <td>
            Satellite Provider
          </td>
          <td>
            {{.Traits.IsSatelliteProvider}}
          </td>
        </tr>
        </tbody>
      </table>  
    </div>
  </div>
</div>
{{template "foot" .}}
{{end}}
